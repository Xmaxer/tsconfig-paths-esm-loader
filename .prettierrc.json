{
  "bracketSpacing": true,
  "singleQuote": true,
  "trailingComma": "all",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^[./]",
    "^@(.*)$",
    "^@src(.*)$",
    "^@utils(.*)$",
    "^@routes(.*)$",
    "^@controllers(.*)$",
    "^@database(.*)$",
    "^@authorization(.*)$",
    "^@constants(.*)$"
  ],
  "importOrderSeparation": true
}
