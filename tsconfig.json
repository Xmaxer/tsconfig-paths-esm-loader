{
  "compilerOptions": {
    "lib": ["ESNext"],
    "moduleResolution": "node16",
    "module": "node16",
    "noUnusedLocals": true,
    "removeComments": false,
    "target": "ES2022",
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "allowJs": true,
    "maxNodeModuleJsDepth": 20,
    "useUnknownInCatchVariables": false
  }
}
